@PercentageClipping
--track0:割合,0,100,0
--track1:方向,1,4,4,1

if obj.track1==1 then --上
	obj.effect("クリッピング", "上", obj.track0 / 100 * obj.h)
elseif obj.track1==2 then --下
	obj.effect("クリッピング", "下", obj.track0 / 100 * obj.h)
elseif obj.track1==3 then --左
	obj.effect("クリッピング", "左", obj.track0 / 100 * obj.w)
elseif obj.track1==4 then --右
	obj.effect("クリッピング", "右", obj.track0 / 100 * obj.w)
end
